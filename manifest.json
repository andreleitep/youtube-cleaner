{
  "manifest_version": 3,
  "name": "YouTube Cleaner",
  "version": "1.0",
  "description": "Remove Shorts, v√≠deos relacionados e telas finais do YouTube.",
  "permissions": ["scripting", "activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}